<?xml version="1.0"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/actionaz-script">
    <sub-class-of type="application/xml"/>
    <comment>Actionaz script file</comment>
    <comment xml:lang="fr">Fichier script d'Actionaz</comment>
    <glob pattern="*.ascr"/>
    <icon name="actionaz"/>
  </mime-type>
  <mime-type type="application/actionaz-code">
    <sub-class-of type="application/ecmascript"/>
    <comment>Actionaz code file</comment>
    <comment xml:lang="fr">Fichier code d'Actionaz</comment>
    <glob pattern="*.acod"/>
    <icon name="actionaz"/>
  </mime-type>
</mime-info>
